@ECHO off
GOTO start

:find_dp0
REM Set dp0 to the directory of this script
SET dp0=%~dp0
EXIT /B

:start
SETLOCAL
REM Initialize the dp0 variable by calling the find_dp0 subroutine
CALL :find_dp0
REM Execute the Node binary located one level up in the node\bin directory and pass all arguments
"%dp0%\..\node\bin\node" %*
